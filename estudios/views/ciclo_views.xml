<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_ciclo_tree" model="ir.ui.view">
        <field name="name">estudios.ciclo.tree</field>
        <field name="model">ciclo</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="models_id" />
            </tree>
        </field>
    </record>

    <record id="view_ciclo_form" model="ir.ui.view">
        <field name="name">estudios.ciclo.form</field>
        <field name="model">ciclo</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="models_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_estudios_ciclo" model="ir.actions.act_window">
        <field name="name">Lista de ciclos</field>
        <field model="res_model">ciclo</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
